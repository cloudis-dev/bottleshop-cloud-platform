{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/delivery/src",
  "projectType": "application",
  "targets": {
    "analyze": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/delivery"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "apps/delivery"
      }
    },
    "format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter format .",
        "cwd": "apps/delivery"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "apps/delivery"
      }
    },
    "genL10n": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter gen-l10n",
        "cwd": "apps/delivery"
      }
    },
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter install",
        "cwd": "apps/delivery"
      }
    },
    "run": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter run --flavor development --target lib/main_development.dart",
        "cwd": "apps/delivery"
      }
    },
    "buildAppbundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build appbundle --flavor production --target lib/main_production.dart",
        "cwd": "apps/delivery"
      }
    },
    "buildIpa": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ipa --flavor production --target lib/main_production.dart",
        "cwd": "apps/delivery"
      }
    },
    "buildPwa": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build web --release --target lib/main_production.dart",
        "cwd": "apps/delivery"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx run delivery:buildAppbundle", "nx run delivery:buildIpa", "nx run deivery:buildPwa"],
        "parallel": false
      }
    }
  },
  "tags": ["scope:flutter", "type:app"]
}
