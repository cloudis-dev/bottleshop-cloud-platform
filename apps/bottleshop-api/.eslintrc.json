{
  "extends": ["../../.eslintrc.json", "plugin:import/typescript"],
  "plugins": ["@nrwl/nx", "import"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "quotes": ["error", "single"],
        "import/no-unresolved": 0
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
