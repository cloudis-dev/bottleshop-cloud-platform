{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin",
  "projectType": "application",
  "targets": {
    "analyze": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/admin"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "apps/admin"
      }
    },
    "format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter format apps/admin/lib/* -o show --set-exit-if-changed"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "apps/admin"
      }
    },
    "genL10n": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter gen-l10n",
        "cwd": "apps/admin"
      }
    },
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter pub get",
        "cwd": "apps/admin"
      }
    },
    "run": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter run",
        "cwd": "apps/admin"
      }
    },
    "buildApk": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build apk --flavor production --target lib/main_production.dart --split-per-abi",
        "cwd": "apps/admin"
      }
    },
    "buildAppbundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build appbundle --flavor production --target lib/main_production.dart",
        "cwd": "apps/admin"
      }
    },
    "buildIos": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios --flavor production --target lib/main_production.dart",
        "cwd": "apps/admin"
      }
    },
    "buildIpa": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ipa --flavor production --target lib/main_production.dart",
        "cwd": "apps/admin"
      }
    },
    "buildWeb":{
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build web --target lib/main_production.dart --release",
        "cwd": "apps/admin"
      }
    }
  },
  "tags": ["scope:flutter", "type:app"]
}
