    {
      "name": "bottleshopdeliveryapp (profile mode)",
      "cwd": "apps/bottleshopdeliveryapp",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "bottleshopdeliveryapp (release mode)",
      "cwd": "apps/bottleshopdeliveryapp",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
