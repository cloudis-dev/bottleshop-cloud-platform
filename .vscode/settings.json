{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "prettier.resolveGlobalModules": true,
  "jestrunner.enableYarnPnpSupport": false,
  "npm.packageManager": "yarn",
  "task.autoDetect": "off",
  "dart.openTestView": ["testRunStart", "testRunStart"],
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "files.refactoring.autoSave": false,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "none",
  "audioCues.lineHasInlineSuggestion": "off",
  "dart.analysisExcludedFolders": [
    "lib/generated, lib/l10n, build"
  ],
  "dart.onlyAnalyzeProjectsWithOpenFiles": true,
  "dart.lineLength": 120,
  "dart.renameFilesWithClasses": "always",
  "dart.flutterHotReloadOnSave": "always",
  "dart.flutterWebRenderer": "html",
  "dart.hotReloadOnSave": "always",
  "dart.normalizeFileCasing": true,
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewHotReloadOnSaveWatcher": true,
  "dart.previewSdkDaps": true,
  "gitlens.defaultGravatarsStyle": "wavatar",
  "gitlens.fileAnnotations.command": "blame",
  "files.defaultLanguage": "${activeEditorLanguage}",
  "html.format.indentHandlebars": true,
  "prettier.eslintIntegration": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.jsxSingleQuote": true,
  "prettier.parser": "typescript",
  "prettier.printWidth": 120,
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.stylelintIntegration": true,
  "prettier.trailingComma": "all",
  "dart.devToolsPort": 9229,
  "dart.openDevTools": "flutter",
  "dart.cliConsole": "terminal",
  "dart.previewCommitCharacters": true,
  "explorer.sortOrder": "type"
}
