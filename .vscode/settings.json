{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "npm.packageManager": "yarn",
  "task.autoDetect": "off",
  "dart.openTestView": [
    "testRunStart",
    "testRunStart"
  ],
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "files.refactoring.autoSave": false,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "none",
  "audioCues.lineHasInlineSuggestion": "off",
  "dart.analysisExcludedFolders": [
    "lib/generated, lib/l10n, build"
  ],
  "dart.onlyAnalyzeProjectsWithOpenFiles": true,
  "dart.lineLength": 120,
  "dart.renameFilesWithClasses": "always",
  "dart.flutterHotReloadOnSave": "always",
  "dart.flutterWebRenderer": "auto",
  "dart.hotReloadOnSave": "always",
  "dart.normalizeFileCasing": true,
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.previewHotReloadOnSaveWatcher": true,
  "dart.previewSdkDaps": true,
  "files.defaultLanguage": "${activeEditorLanguage}",
  "html.format.indentHandlebars": true,
  "prettier.eslintIntegration": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.jsxSingleQuote": true,
  "prettier.parser": "typescript",
  "prettier.printWidth": 120,
  "prettier.requireConfig": true,
  "prettier.singleQuote": true,
  "prettier.stylelintIntegration": true,
  "prettier.trailingComma": "all",
  "dart.devToolsPort": 9229,
  "dart.openDevTools": "flutter",
  "dart.cliConsole": "terminal",
  "dart.previewCommitCharacters": true,
  "explorer.sortOrder": "type",
  "dart.devToolsLocation": "active",
  "dart.flutterAdbConnectOnChromeOs": true,
  "dart.debugExternalPackageLibraries": true,
  "dart.projectSearchDepth": 2,
  "debug.terminal.clearBeforeReusing": true,
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.detectLocale": "off",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.localEchoEnabled": "on",
  "terminal.integrated.macOptionClickForcesSelection": true,
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.minimumContrastRatio": 7,
  "terminal.integrated.persistentSessionReviveProcess": "onExitAndWindowClose",
  "terminal.integrated.rightClickBehavior": "copyPaste",
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.splitCwd": "workspaceRoot",
  "code-runner.runInTerminal": true,
  "python.terminal.activateEnvInCurrentTerminal": true,
  "editor.find.seedSearchStringFromSelection": "never",
  "merge-conflict.decorators.enabled": false,
  "code-runner.enableAppInsights": false,
  "nxConsole.enableTelemetry": false,
  "mdb.sendTelemetry": false,
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 16,
  "editor.fontWeight": "normal",
  "editor.fontLigatures": true,
  "workbench.productIconTheme": "Default",
  "window.autoDetectColorScheme": true,
  "window.closeWhenEmpty": true,
  "search.useParentIgnoreFiles": true,
  "debug.allowBreakpointsEverywhere": true,
  "debug.autoExpandLazyVariables": true,
  "debug.console.closeOnEnd": true,
  "testing.automaticallyOpenPeekViewDuringAutoRun": false,
  "debug.console.fontFamily": "M",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
}
